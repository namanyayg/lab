extends templates/_layout

block vars

block title
  title Lab | Namanyay Goel

block content
  header
    h1 Lab
    h3.credits By <a href="http://nmn.gl/">Namanyay Goel</a>

  .creations
    - for (var i = 0; i < data.length; i++) {
      - var c = data[i]
      .creation(class="#{c.key}" data-type="#{c.type}")
        a(href=c.href)
          .creation__overlay
          .creation__img
            img(src="img/#{c.key}.png")
          .creation__text
            .creation__name #{c.name} <span class="type">#{c.type}</span>
    - }

  script.
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

    ga('create', 'UA-41471024-6', 'auto');
    ga('send', 'pageview');
